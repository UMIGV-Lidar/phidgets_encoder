<package format = "2">
	<name>phidgets_encoder</name>
	<version>0.1.0</version>
	<description>
		Publishes a nav_msgs/Odometry message based on wheel encoder data.
	</description>
	<maintainer email="gregjm@umich.edu">Gregory Meyer</maintainer>
	<license>BSD 3-Clause License</license>

	<url type="repository">https://github.com/umigv/phidgets_encoder</url>
	<author>Gregory Meyer</author>

	<buildtool_depend>catkin</buildtool_depend>

	<depend>roscpp</depend>
	<depend>phidgets_api</depend>
	<depend>nav_msgs</depend>
	<depend>tf</depend>

	<build_depend>libboost-dev</build_depend>
	<build_depend>ros_cpp11_range</build_depend>
</package>